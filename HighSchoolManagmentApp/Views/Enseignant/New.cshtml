@model HighSchoolManagmentApp.ViewModel.EnseignantViewModel
@{
    ViewData["Title"] = "New";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>New</h2>
@using (Html.BeginForm("create", "enseignant"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.enseignant.Name)
        @Html.TextBoxFor(m => m.enseignant.Name, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.enseignant.FamilyName)
        @Html.TextBoxFor(m => m.enseignant.FamilyName, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.enseignant.Subjects)
        @Html.DropDownListFor(m => m.enseignant.Subjects, new SelectList(Model.subjects, "Id", "Name"), "Select Seance", new { @class = "form-control" })
    </div>
   
    <button type="submit" class=" btn btn_primary"> save </button>
}
